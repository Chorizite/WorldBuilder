<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:views="using:WorldBuilder.Views"
             mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="300"
             x:Class="WorldBuilder.Views.DataIdPreview"
             x:Name="Root">
    <views:DatObjectPreview 
        DataId="{Binding DataId, ElementName=Root}"
        Dats="{Binding Dats, ElementName=Root}"
        IsTooltip="{Binding IsTooltip, ElementName=Root}" />
</UserControl>
